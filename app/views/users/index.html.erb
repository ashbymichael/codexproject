<form id="github-search" method="get" action="https://api.github.com/search/repositories" >
  Enter your query: <input name="q" type="text">
  <input value="submit this" type="submit">
  <div id="github-search-stuff">
    <script type="text/html" id="search-results">
      <div data-content="author"></div>
      <div data-content="date"></div>
      <img data-src="authorPicture" data-alt="author"/>
      <div data-content="post"></div>
  </script>
  </div>
<div id='template-container'>
  <script type="text/html" id="search-results">
      <div data-content="author0"></div>
      <div data-content="date"></div>
      <img data-src="authorPicture" data-alt="author"/>
      <div data-content="post"></div>
      <div data-content="repository"></div>
  </script>
</div>
</form>
